@model SuppliersProducts.Models.StudentLabWork
@{
    ViewData["Title"] = "Compare";
}

<h1>Compare</h1>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Compare" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Path" class="control-label"></label>
                <select name="fp1" asp-for="Path" class="form-control" asp-items="ViewBag.Path"></select>
                <span asp-validation-for="Path" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Path" class="control-label"></label>
                <select name="fp2" asp-for="Path" class="form-control" asp-items="ViewBag.Path"></select>
                <span asp-validation-for="Path" class="text-danger"></span>
            </div>
            <div>
               @ViewBag.UltimateRes
            </div>
            <div class="form-group">
                <input type="submit" value="Compare" class="btn btn-primary" />
            </div>
            <a asp-action="Index">Back to List</a>
        </form>
    </div>
</div>

